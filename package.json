{
  "name": "instaminsta-blog-api",
  "version": "1.0.0",
  "description": "SEO Blog Automation API for Instaminsta",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "node node_modules/vite/bin/vite.js",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"cd backend && npm start\"",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "migrate": "node scripts/migrate-database.js",
    "seed": "node scripts/seed-keywords.js"
  },
  "keywords": [
    "seo",
    "blog",
    "automation",
    "n8n",
    "instagram"
  ],
  "author": "Instaminsta Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.5",
    "dotenv": "^16.3.1",
    "archiver": "^7.0.1",
    "axios": "^1.13.4",
    "puppeteer": "^21.5.2",
    "marked": "^11.1.0",
    "dompurify": "^3.0.6",
    "jsdom": "^23.0.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "lucide-react": "^0.303.0",
